config wifi-device 'radio0'
	option type 'mac80211'
	option channel '10'
	option hwmode '11g'
	option path 'pci0000:00/0000:00:01.0/0000:02:00.0'
	option htmode 'HT20'
	option country 'US'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option mode 'ap'
	option network 'priv'
	option encryption 'psk2'
	option key ''
	option ssid 'WepaNet-priv'

config wifi-device 'radio1'
	option type 'mac80211'
	option channel '40'
	option hwmode '11a'
	option path 'pci0000:00/0000:00:00.0/0000:01:00.0'
	option htmode 'VHT80'
	option country 'US'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option mode 'ap'
	option network 'pub'
	option encryption 'psk2'
	option key ''
	option ssid 'LolNet'

config wifi-iface 'mesh_five'
	option device 'radio1'
	option mode 'mesh'
	option mesh_fwding '0'
	option mesh_id 'ZD4Pbfeu3Ob6dGzJ'
	option network 'mesh'
	option encryption 'psk2+ccmp'
	option key ''
	option ifname 'mesh1'
