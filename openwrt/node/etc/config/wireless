
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/10180000.wmac'
	option htmode 'HT40'
	option noscan '1'
	option short_preamble '1'
	option disabled '0'
	option country 'US'
	option channel '10'
	option txpower '23'

config wifi-iface 'ap_two'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ifname 'wlan0'
	option hidden '0'
	option disassoc_low_ack '0'
	option encryption 'psk2'
	option ft_over_ds '1'
	option ft_psk_generate_local '1'
	option ieee80211r '1'
	option ssid 'WepaNet2G'
	option key ''

config wifi-device 'radio1'
	option type 'mac80211'
	option hwmode '11a'
	option path 'pci0000:00/0000:00:00.0/0000:01:00.0'
	option noscan '1'
	option short_preamble '1'
	option disabled '0'
	option country 'US'
	option txpower '30'
	option channel '40'
	option htmode 'VHT80'

config wifi-iface 'ap_five'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ifname 'wlan1'
	option hidden '0'
	option encryption 'psk2'
	option ft_over_ds '1'
	option ft_psk_generate_local '1'
	option ieee80211r '1'
	option ssid 'WepaNetP'
	option key ''

config wifi-iface 'mesh_five'
	option ifname 'mesh1'
	option device 'radio1'
	option hidden '0'
	option encryption 'psk2+ccmp'
	option network 'mesh'
	option mode 'mesh'
	option mesh_fwding '0'
	option mcast_rate '18000'
	option mesh_id 'ZD4Pbfeu3Ob6dGzJ'
	option key ''

