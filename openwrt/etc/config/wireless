config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/10180000.wmac'
	option htmode 'HT40'
	option noscan '1'
	option short_preamble '1'
	option disabled '0'
	option country 'US'
	option channel '6'
	option txpower '23'

config wifi-iface 'ap_two'
	option device 'radio0'
	option network 'pub'
	option mode 'ap'
	option bssid ''
	option ifname 'wlan0'
	option hidden '0'
	option disassoc_low_ack '0'
	option ssid 'WepaNet'
	option encryption 'psk2'
	option key ''
	option nasid ''
	option r1_key_holder ''
	option ft_over_ds '1'
	option ft_psk_generate_local '1'
	option ieee80211r '1'

config wifi-device 'radio1'
	option type 'mac80211'
	option hwmode '11a'
	option path 'pci0000:00/0000:00:00.0/0000:01:00.0'
	option noscan '1'
	option short_preamble '1'
	option disabled '0'
	option country 'US'
	option txpower '30'
	option channel '40'
	option htmode 'VHT80'

config wifi-iface 'ap_five'
	option device 'radio1'
	option network 'priv'
	option mode 'ap'
	option bssid ''
	option ifname 'wlan1'
	option hidden '0'
	option encryption 'psk2'
	option ssid 'WepaNet-priv'
	option key ''
	option ft_over_ds '1'
	option ft_psk_generate_local '1'
	option ieee80211r '1'

